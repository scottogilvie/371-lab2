    %end;
    .thread T_1;
    .scope S_0037dee8;
T_2 ;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0065dd30_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0065dc28_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0065dc28_0, 0, 1;
    %pushi/vec4 0, 0, 9;
    %store/vec4 v0065db78_0, 0, 9;
T_2.0 ;
    %load/vec4 v0065db78_0;
    %cmpi/u 255, 0, 9;
    %flag_or 5, 4;
    %jmp/0xz T_2.1, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0065dc80_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0065dcd8_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0065db78_0;
    %parti/s 8, 0, 2;
    %store/vec4 v0065dac8_0, 0, 8;
    %delay 5, 0;
    %load/vec4 v0065dd30_0;
    %store/vec4 v0065dbd0_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0065dcd8_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0065dcd8_0, 0, 1;
    %load/vec4 v0065dd30_0;
    %subi 1, 0, 8;
    %store/vec4 v0065dd30_0, 0, 8;
    %load/vec4 v0065db78_0;
    %addi 1, 0, 9;
    %store/vec4 v0065db78_0, 0, 9;
    %jmp T_2.0;
T_2.1 ;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0065dd30_0, 0, 8;
    %pushi/vec4 0, 0, 9;
    %store/vec4 v0065db78_0, 0, 9;
T_2.2 ;
    %load/vec4 v0065db78_0;
    %cmpi/u 255, 0, 9;
    %flag_or 5, 4;
    %jmp/0xz T_2.3, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0065dcd8_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0065dc80_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0065db78_0;
    %parti/s 8, 0, 2;
    %store/vec4 v0065dac8_0, 0, 8;
    %delay 10, 0;
    %load/vec4 v0065db20_0;
    %load/vec4 v0065dd30_0;
    %cmp/ne;
    %jmp/0xz  T_2.4, 4;
    %vpi_call 2 93 "$display", "Error: data: %h != expected %h at time %d\012", v0065db20_0, v0065dd30_0, $time {0 0 0};
T_2.4 ;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0065dc80_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0065dd30_0;
    %subi 1, 0, 8;
    %store/vec4 v0065dd30_0, 0, 8;
    %load/vec4 v0065db78_0;
    %addi 1, 0, 9;
    %store/vec4 v0065db78_0, 0, 9;
    %jmp T_2.2;
T_2.3 ;
    %delay 1300, 0;
    %vpi_call 2 102 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0037fe48;
T_3 ;
